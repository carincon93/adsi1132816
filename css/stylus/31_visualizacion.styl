body
	color #999
	font 10px Arial
section.box
	border 2px solid #ccc
	height 330px
	padding 10px
	width 330px
section.box div
	border 2px solid #ccc
	display inline-block
	height 100px
	line-height 60px
	margin-bottom 10px
	text-align center
	width 100px
	
section div.boxmostrar
	display none
section div.boxvisible
	visibility hidden

main.tipos div,
main.tipos a
	border 2px solid #999
	padding 10px
	margin-bottom 10px
main.tipos div.linea
	display inline
main.tipos a.bloque
	display block
	