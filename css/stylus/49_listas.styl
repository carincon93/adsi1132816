body
	background-color #FFBCBC
	font 16px Tahoma
section
	background-color #fff
	margin 20px auto
	padding 20px
	width 800px
ul
	border 2px dotted #ED3636
	display inline-block
	margin 10px
	vertical-align top
ul.disc li
	list-style-type disc
ul.circle li
	list-style-type circle
	list-style-position inside
ul.square li
	list-style-type square
	
ol.decimal li
	list-style-type decimal
ol.roman li
	list-style-type upper-roman
	list-style-position inside
ol.greek li
	list-style-type lower-greek
	
ul.imagenes li
	list-style-image url(../imgs/globe.jpg)
/* Forma Corta */
ul.imagenes li
	list-style decimal outside url(../imgs/globe.jpg)