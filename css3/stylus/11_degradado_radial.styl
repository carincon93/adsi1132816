section
    border 4px solid #333
    height 300px
    margin 10px auto
    width 600px
.radial1
    background -webkit-radial-gradient(#f00, #ff0)
.radial2
    background -webkit-radial-gradient(#ff0, #00f, #f00)
.radial3
    background -webkit-radial-gradient(#ff0 5%, #00f 15%, #f00 60%)
.radial4
    background -webkit-radial-gradient(circle, #fff, #f00)